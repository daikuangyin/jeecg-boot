<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.recycle.mapper.RecycleUserMapper">
    <select id="getUserById" parameterType="java.lang.String" resultType="org.jeecg.modules.recycle.entity.RecycleUser">
        select
            *
         from
         recycle_user
         where uid = #{userId}
    </select>
</mapper>